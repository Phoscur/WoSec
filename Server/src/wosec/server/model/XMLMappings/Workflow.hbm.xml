<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="wosec.server.model">
 <class entity-name="XMLWorkflow" node="workflow" table="Workflows">
     <id name="workflow-id" column="id" type="string" node="@workflow-id" />
     <property name="workflow-name" column="name" type="string" node="@workflow-name" />
 </class>
</hibernate-mapping>